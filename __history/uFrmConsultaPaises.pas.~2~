  private
    { Private declarations }
  public
    { Public declarations }
    procedure ConhecaObj( pObj, pCtrl : TObject );  override;
    procedure Pesquisar;                            override;
    procedure Incluir;                              override;
    procedure Alterar;                              override;
    procedure Excluir;                              override;
    procedure Sair;                                 override;
  end;

